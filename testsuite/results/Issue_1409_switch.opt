




	INCLUDE "z80_crt0.hdr"


	SECTION	code_compiler

._func
	pop	bc
	pop	hl
	push	hl
	push	bc
	call	l_case
	defw	i_4
	defw	1
	defw	i_5
	defw	3
	defw	0
	jp	i_3	;EOS
.i_4
	ld	hl,1	;const
	push	hl
	call	_dofunc
	pop	bc
	jp	i_3	;EOS
.i_5
	ld	hl,1	;const
	push	hl
	call	_dofunc
	pop	bc
.i_3
	ret




	SECTION	bss_compiler
	SECTION	code_compiler



	GLOBAL	_dofunc
	GLOBAL	_func




