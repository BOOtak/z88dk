	ex	de,hl
	ld	bc,56534
	push	bc
	ld	bc,20480
	push	bc
	call	l_long_div_u
	call	l_long_mult
	push	de
	push	hl
	ld	hl,100	;const
	ld	de,0
	call	l_long_div_u
	ret




